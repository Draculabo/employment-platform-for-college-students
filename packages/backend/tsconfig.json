{
  "extends": "fastify-tsconfig",
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    },
  },
  "compilerOptions": {
    "module": "ESNext",
    "outDir": "dist",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "lib": [
      "ES6",
      "es2018"
    ],
    "target": "es2018",
    "types": [
      // "vite/client"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "./types/*.ts",
  ]
}